    {
      "type": "vcs",
      "url": "git@github.com:linkslegend/indeed-ultimate-affiliate-pro.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:linkslegend/woocommerce-quick-view.git"
    },
